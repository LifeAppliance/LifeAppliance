<!DOCTYPE html>
<html>
  <head>
    <title>LifeAppliance</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Favicons -->
    <%= favicon_link_tag('logo/apple-icon-57x57.png' , rel:'apple-touch-icon' , sizes: '57x57') %>
    <%= favicon_link_tag('logo/apple-icon-60x60.png' , rel:'apple-touch-icon' , sizes: '60x60') %>
    <%= favicon_link_tag('logo/apple-icon-72x72.png' , rel:'apple-touch-icon' , sizes: '72x72') %>

    <%= favicon_link_tag('logo/apple-icon-114x114.png' , rel:'apple-touch-icon' , sizes: '114x114') %>
    <%= favicon_link_tag('logo/apple-icon-120x120.png' , rel:'apple-touch-icon' , sizes: '120x120') %>
    <%= favicon_link_tag('logo/apple-icon-144x144.png' , rel:'apple-touch-icon' , sizes: '144x144') %>
    <%= favicon_link_tag('logo/apple-icon-152x152.png' , rel:'apple-touch-icon' , sizes: '152x152') %>
    <%= favicon_link_tag('logo/apple-icon-180x180.png' , rel:'apple-touch-icon' , sizes: '180x180') %>
    <%= favicon_link_tag('logo/android-icon-192x192.png' , rel:'icon' , sizes:'192x192' , type:'image/png') %>
    <%= favicon_link_tag('logo/android-icon-96x96.png' , rel:'icon' , sizes:'96x96' , type:'image/png') %>
    <!-- End -->

  </head>

  <body>
    <%= yield %>
  </body>
</html>
